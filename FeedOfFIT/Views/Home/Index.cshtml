@{
    ViewBag.Title = "Index";
}

<h2>@ViewBag.RssTitle</h2>

@foreach (var item in ViewBag.NewsList)
{
    <div class="content">
        <a href="@item.Link">@item.Title</a>
        <div>@item.CreateAt.ToShortDateString()</div>
    </div>
    
}
<div> <a href="~/Home/RssResult"> <img src="~/Images/rss.gif" width="40" height="40"/></a> </div>
